{
  "name": "officebot-sdk",
  "version": "2.0.17",
  "description": "Helper library for interacting with RESTful APIs",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "src": "src",
    "test": "tests"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack --config webpack.dev.js",
    "coverage": "node_modules/nyc/bin/nyc.js --reporter=html npm test",
    "docs": "node_modules/jsdoc/jsdoc.js  -r -c .jsdoc.json -R sdk-examples.md -d docs",
    "test": "node_modules/jshint/bin/jshint src/*.js && node_modules/mocha/bin/mocha tests/*.js && node ./run-e2e",
    "preversion": "npm test",
    "version": "git add -A && npm run build && npm run build-minified",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "rest",
    "javascript"
  ],
  "author": "Scott Peterson <scott.peterson@office-bot.com>",
  "license": "MIT",
  "dependencies": {
    "clone": "^2.1.1",
    "fast-json-patch": "^2.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-spies": "^0.7.1",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.2",
    "jsdoc": "^3.5.5",
    "jshint": "^2.9.5",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.4",
    "latodoc": "github:smeijer/latodoc",
    "minami": "^1.2.3",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "path": "^0.12.7",
    "uglifyify": "^4.0.4",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-merge": "^4.1.2"
  }
}
